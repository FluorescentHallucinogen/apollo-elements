{
  "name": "lit-apollo",
  "version": "0.7.2",
  "description": "A set of base classes to help create custom elements connected to an ApolloClient cache",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "postinstall": "patch-package",
    "build": "rollup -c",
    "dev": "nodemon -x 'npm test'",
    "predev": "npm run build",
    "pretest": "forever start test/forever.json",
    "posttest": "forever stopall",
    "test": "karma start karma.conf.js"
  },
  "logo": {
    "file": "./logo.png"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bennypowers/lit-apollo.git"
  },
  "keywords": [
    "Apollo",
    "GraphQL",
    "Web Components",
    "Custom Elements",
    "LitElement",
    "lit-html"
  ],
  "author": "Benny Powers <web@bennypowers.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bennypowers/lit-apollo/issues"
  },
  "homepage": "https://github.com/bennypowers/lit-apollo#readme",
  "dependencies": {
    "@polymer/lit-element": "^0.6.4",
    "apollo-client": "^2.4.7",
    "crocks": "^0.9.4",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2"
  },
  "devDependencies": {
    "@open-wc/testing": "^0.7.0",
    "@open-wc/testing-karma": "^0.2.3",
    "@types/graphql": "^14.0.3",
    "@types/jsdom": "^12.2.0",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-link-http": "^1.5.7",
    "apollo-link-schema": "^1.1.2",
    "apollo-link-ws": "^1.0.10",
    "apollo-server": "^2.2.6",
    "diff": "^3.5.0",
    "eslint": "^4.19.1",
    "eslint-config-blink": "0.1.0",
    "eslint-config-google": "^0.9.1",
    "eslint-config-recommended": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "esm": "^3.0.84",
    "forever": "^0.15.3",
    "graphql-tools": "^4.0.3",
    "jsdom": "^13.0.0",
    "karma": "^3.1.3",
    "karma-mocha": "^1.3.0",
    "mocha": "^5.2.0",
    "node-fetch": "^2.3.0",
    "nodemon": "^1.18.8",
    "npm-run-all": "^4.1.5",
    "patch-package": "^5.1.1",
    "rollup-plugin-commonjs": "^8.3.4",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-string": "^2.0.2",
    "sinon": "^7.2.0",
    "sinon-chai": "^3.3.0",
    "subscriptions-transport-ws": "^0.9.15",
    "ws": "^6.1.2"
  }
